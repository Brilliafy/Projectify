{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@db/*": [
        "src/generated/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}